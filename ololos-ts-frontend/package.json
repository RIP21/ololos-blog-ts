{
  "name": "ololos-ts-frontend",
  "version": "0.1.0",
  "description": "ololos-ts-frontend",
  "license": "MIT",
  "engines": {
    "node": "6"
  },
  "scripts": {
    "prebuild": "npm run clean",
    "build": "cross-env NODE_ENV=production webpack -p",
    "clean": "rimraf dist/",
    "dev:client": "cross-env NODE_ENV=development webpack-dev-server -d --inline --hot --progress --no-info",
    "start": "npm run dev:client",
    "lint": "npm run lint-ts && npm run lint-js && npm run lint-css",
    "lint-css": "stylelint \"./src/**/*.css\"",
    "lint-ts": "tslint \"src/**/*.tsx\" \"src/**/*.ts\" \"test/**/*.tsx\" \"test/**/*.ts\"",
    "lint-js": "eslint \"**/*.js\" --ignore-path .gitignore",
    "postinstall": "npm run typings",
    "pretest": "npm run lint",
    "test": "",
    "test:watch": "",
    "typings": "rimraf typings/ && typings install"
  },
  "keywords": [
    "react",
    "redux",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": ""
  },
  "dependencies": {
    "react": "^15.3.2",
    "react-addons-test-utils": "^15.3.0",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.4",
    "react-router": "^2.8.1",
    "react-router-redux": "^4.0.5",
    "redux": "^3.4.0",
    "redux-form": "^5.3.2",
    "redux-localstorage": "^0.4.0",
    "redux-logger": "^2.6.0",
    "redux-thunk": "^2.1.0"
  },
  "devDependencies": {
    "express": "^4.13.4",
    "helmet": "^2.0.0",
    "http-proxy": "^1.13.2",
    "winston": "^2.1.1",
    "autoprefixer": "^6.3.3",
    "awesome-typescript-loader": "^1.0.0",
    "babel-core": "^6.9.1",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "basscss": "^8.0.1",
    "basscss-addons": "^1.0.0",
    "basscss-basic": "^1.0.0",
    "body-parser": "^1.15.1",
    "classnames": "^2.2.3",
    "concurrently": "^2.0.0",
    "copy-webpack-plugin": "^3.0.1",
    "cross-env": "^2.0.0",
    "css-loader": "^0.23.1",
    "cssnano": "^3.5.2",
    "denodeify": "^1.2.1",
    "enzyme": "^2.4.0",
    "eslint": "^3.2.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "history": "^3.0.0",
    "html-webpack-plugin": "^2.9.0",
    "immutable": "^3.8.1",
    "imports-loader": "^0.6.5",
    "json-loader": "^0.5.4",
    "nodemon": "^1.9.2",
    "object-assign": "^4.0.1",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "postcss-cssnext": "^2.4.0",
    "postcss-import": "^8.0.2",
    "postcss-loader": "^0.9.1",
    "postcss-modules-local-by-default": "^1.0.2",
    "raw-loader": "^0.5.1",
    "require-hacker": "^2.1.3",
    "rimraf": "^2.4.3",
    "source-map-support": "^0.4.0",
    "style-loader": "^0.13.0",
    "stylelint": "^6.8.0",
    "stylelint-webpack-plugin": "^0.2.0",
    "ts-helpers": "^1.1.1",
    "tslint": "^3.7.4",
    "tslint-loader": "^2.1.0",
    "typescript": "^1.8.10",
    "typings": "^1.0.4",
    "url-loader": "^0.5.6",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.12.0",
    "webpack-split-by-path": "0.0.10",
    "whatwg-fetch": "^1.0.0"
  }
}
